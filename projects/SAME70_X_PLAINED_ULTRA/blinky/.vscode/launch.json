    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/blinky_asf.elf",
            "name": "Debug blinky_SAME70",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "jlink",
            "armToolchainPath": "${env:ARM_NONE_EABI_TOOLCHAIN}/bin",
            "device": "ATSAME70Q21B",
            "interface": "swd",
            "serialNumber": "", // add J-Link serial number if having multiple attached the same time.
            "runToEntryPoint": "main",
            "breakAfterReset": true,
            "svdFile": "${workspaceFolder}/../../../tools/svd/ATSAME70Q21B.svd",

        },
    ]
}